#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
puts "Welcome to Tic Tac Toe!\n"
board = [" "," "," "," "," "," "," "," "," "]
puts "Where would you like to go?\n"
input = gets.strip
index = input_to_index(input)

p board
p index
move (board, index)
display_board(board)
